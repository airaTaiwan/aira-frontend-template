{
  "name": "aira-app",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.4.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:arch",
    "build:arch": "npx vite-node scripts/arch-dist.ts",
    "upload:internal": "for REMOTE in '51';  do scp -r dist/* $REMOTE:/opt/aira/airaApp/www/; done",
    "deploy": "pnpm build && pnpm upload",
    "deploy:internal": "pnpm build && pnpm upload:internal",
    "preview": "vite preview",
    "release": "bumpp --commit --tag --push",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint 'src/**/*.{ts,vue}'",
    "lint:fix": "eslint --fix .",
    "up": "taze major -I"
  },
  "dependencies": {
    "@pinia/colada": "^0.13.6",
    "@vueuse/core": "^12.7.0",
    "@vueuse/integrations": "^12.7.0",
    "date-fns": "^4.1.0",
    "ky": "^1.7.5",
    "pinia": "^3.0.1",
    "universal-cookie": "^7.2.2",
    "v-scale-screen": "^2.3.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-step-progress": "^1.0.2",
    "vxe-pc-ui": "^4.3.88",
    "vxe-table": "^4.10.15"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.2.1",
    "@coreui/coreui-pro": "^5.9.0",
    "@coreui/icons": "^3.0.1",
    "@coreui/icons-vue": "^2.2.0",
    "@coreui/vue-pro": "^5.10.0",
    "@iconify-json/carbon": "^1.2.7",
    "@iconify-json/cil": "^1.2.2",
    "@intlify/unplugin-vue-i18n": "^6.0.3",
    "@types/archiver": "^6.0.3",
    "@types/node": "^22.13.4",
    "@unocss/eslint-config": "^65.5.0",
    "@unocss/eslint-plugin": "^65.5.0",
    "@unocss/reset": "^65.5.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "archiver": "^7.0.1",
    "bumpp": "^10.0.3",
    "eslint": "^9.20.1",
    "eslint-plugin-format": "^1.0.1",
    "lint-staged": "^15.4.3",
    "pnpm": "^10.4.1",
    "sass": "^1.85.0",
    "simple-git-hooks": "^2.11.1",
    "taze": "^18.5.0",
    "typescript": "~5.7.3",
    "unocss": "^65.5.0",
    "unocss-preset-scrollbar": "^3.2.0",
    "unplugin-auto-import": "^19.1.0",
    "unplugin-vue-components": "^28.1.0",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^6.1.0",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue-component-type-helpers": "^2.2.2",
    "vue-i18n": "^11.1.1",
    "vue-tsc": "^2.2.2"
  },
  "resolutions": {
    "vite": "^6.1.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
