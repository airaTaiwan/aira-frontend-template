{
  "name": "aira-app",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.8.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:arch",
    "build:arch": "esno scripts/arch-dist.ts",
    "upload:internal": "for REMOTE in '51';  do scp -r dist/* $REMOTE:/opt/aira/airaApp/www/; done",
    "deploy": "pnpm build && pnpm upload",
    "deploy:internal": "pnpm build && pnpm upload:internal",
    "preview": "vite preview",
    "release": "bumpp --commit --tag --push",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint 'src/**/*.{ts,vue}'",
    "lint:fix": "eslint --fix .",
    "up": "taze major -I"
  },
  "dependencies": {
    "@pinia/colada": "^0.14.2",
    "@vueform/multiselect": "^2.6.11",
    "@vuepic/vue-datepicker": "^11.0.2",
    "@vueuse/core": "^13.1.0",
    "@vueuse/integrations": "^13.1.0",
    "date-fns": "^4.1.0",
    "ky": "^1.8.1",
    "pinia": "^3.0.2",
    "universal-cookie": "^8.0.1",
    "v-scale-screen": "^2.3.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-step-progress": "^1.0.2",
    "vxe-pc-ui": "^4.5.23",
    "vxe-table": "^4.13.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@coreui/coreui-pro": "^5.9.0",
    "@coreui/icons": "^3.0.1",
    "@coreui/icons-vue": "^2.2.0",
    "@coreui/vue-pro": "^5.10.0",
    "@iconify-json/carbon": "^1.2.8",
    "@iconify-json/cil": "^1.2.2",
    "@intlify/unplugin-vue-i18n": "^6.0.5",
    "@types/archiver": "^6.0.3",
    "@types/node": "^22.14.1",
    "@unocss/eslint-config": "^66.1.0-beta.11",
    "@unocss/eslint-plugin": "^66.1.0-beta.11",
    "@unocss/reset": "^66.1.0-beta.11",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue.ts/complex-types": "^0.6.0",
    "archiver": "^7.0.1",
    "bumpp": "^10.1.0",
    "eslint": "^9.24.0",
    "eslint-plugin-format": "^1.0.1",
    "esno": "^4.8.0",
    "lint-staged": "^15.5.1",
    "pnpm": "^10.8.1",
    "sass": "^1.86.3",
    "simple-git-hooks": "^2.12.1",
    "taze": "^19.0.4",
    "typescript": "~5.8.3",
    "unocss": "^66.1.0-beta.11",
    "unocss-preset-scrollbar": "^3.2.0",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-vue-components": "^28.5.0",
    "unplugin-vue-router": "^0.12.0",
    "vite": "link:@vue.ts/complex-types/vite",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue-component-type-helpers": "^2.2.8",
    "vue-i18n": "^11.1.3",
    "vue-tsc": "^2.2.8"
  },
  "resolutions": {
    "vite": "^6.2.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
